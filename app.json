{
  "expo": {
    "name": "githubExplorer",
    "slug": "githubExplorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "proguardEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "statusBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content",
        "translucent": false
      },
      "package": "com.anjy.githubExplorer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "09a1a2a0-be5c-46ad-8db5-c1f7805ab674"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-build-properties"
    ],
    "updates": {
      "url": "https://u.expo.dev/09a1a2a0-be5c-46ad-8db5-c1f7805ab674"
    }
  }
}
